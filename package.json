{
  "name": "docker-run",
  "displayName": "docker-run",
  "description": "This extension will help you to automatically start docker containers",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.45.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:**/.dockerrc"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "docker-run.start:all",
        "title": "Docker Run: Start All"
      },
      {
        "command": "docker-run.stop:all",
        "title": "Docker Run: Stop All"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/dockerode": "^2.5.29",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.45.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "dockerode": "^3.2.0"
  }
}
